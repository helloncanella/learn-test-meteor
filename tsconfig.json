{
  "atom": {
    "rewriteTsconfig": true
  },
  "compileOnSave": false,
  "buildOnSave": false,
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "jsx": "react"
  },
  "filesGlob": [
    "client/**/*.tsx",
    "client/**/*.ts",
    "typings/**/*.ts",
    "imports/**/*.ts",
    "server/**/*.ts"
  ],
  "files": [
    "server/main.ts",
    "imports/api/saveData.ts",
    "client/Contact/Contact.tsx",
    "client/ContactForm/ContactForm.tsx",
    "client/ContactListContainer/ContactListContainer.tsx",
    "client/main.tsx",
    "typings/globals/meteor/index.d.ts",
    "typings/globals/react-dom/index.d.ts",
    "typings/globals/react/index.d.ts",
    "typings/index.d.ts"
  ]
}
